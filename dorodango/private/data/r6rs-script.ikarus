#!/bin/sh

export IKARUS_LIBRARY_PATH="@R6RS_LIBRARY_PATH@"

while true; do
    case "$1" in
        --compile)
            shift
            exec ikarus --compile-dependencies "$@"
            ;;
        --)
            shift
            break
            ;;
        *)
            break
            ;;
    esac
done

exec ikarus --r6rs-script "$@"
